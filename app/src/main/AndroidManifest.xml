<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.huawei.faceoffdresstrial">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/app_icon_new"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">
        <activity android:name=".EntryChoiceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".kotlin.MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".kotlin.DressTrialActivity" />
        <activity
            android:name=".kotlin.ImageCaptureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".kotlin.ImageCropActivity"
            android:screenOrientation="portrait" />


        <activity
            android:name=".java.MainActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".java.DressTrialActivity" />
        <activity
            android:name=".java.ImageCaptureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".java.ImageCropActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>